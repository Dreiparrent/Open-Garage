{
    "name": "opengarage",
    "version": "0.0.0",
    "license": "MIT",
    "bin": {
        "dot": "../Graphviz2.38/bin"
    },
    "config": {
        "port": "4200"
    },
    "scripts": {
        "ng": "ng",
        "server-dev": "webpack-dev-server --progress --config webpack.dev.js --port %npm_package_config_port%",
        "server-prod": "webpack-dev-server --progress --config webpack.prod.js --port %npm_package_config_port%",
        "webpack-build": "rimraf dist && webpack --config webpack.prod.js --progress",
        "start": "nodemon -w webpack.common.js -w webpack.dev.js -x npm run server-dev",
        "start:prod": "nodemon -w webpack.common.js -webpack.prod.js -x npm run server-prod",
        "start:build": "nodemon -w webpack.common.js -w webpack.prod.js -x npm run webpack-build && angular-http-server --path dist",
        "build:jit": "nodemon -w webpack.common.js -w webpack.prod.js -x npm run webpack-build",
        "aot": "rimraf dist/aot && webpack --config webpack.aot.js --progress",
        "stats": "webpack --config webpack.prod.js --json > stats.json",
        "build": "rimraf dist/aot && webpack --config webpack.aot.js --progress --verbose",
        "build:stats": "set STATS=true&& rimraf dist/aot && webpack --config webpack.aot.js --progress --verbose --json > stats.json && webpack-bundle-analyzer -- stats.json",
        "build:fast": "set FAST=true&& rimraf dist/aot && webpack --config webpack.aot.js --progress",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.0.0-beta.2",
